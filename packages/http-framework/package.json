{
  "name": "@skyra/http-framework",
  "version": "0.0.1",
  "description": "The framework for ArchId Network's HTTP-only bots",
  "scripts": {
    "build": "tsup && tsc --emitDeclarationOnly --incremental",
    "lint": "prettier --check . && eslint src --ext mjs,js,ts",
    "format": "prettier --write . && eslint src --ext mjs,js,ts --fix",
    "prepublishOnly": "yarn build && yarn lint && yarn test"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "typings": "./dist/index.d.ts",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.js"
  },
  "directories": {
    "lib": "src"
  },
  "files": [
    "dist"
  ],
  "contributors": [
    "Antonio Rom√°n <kyradiscord@gmail.com>",
    "Jeroen Claassens <support@favware.tech>"
  ],
  "license": "Apache-2.0",
  "keywords": [
    "discord",
    "api",
    "http"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/skyra-project/archid-components.git"
  },
  "bugs": {
    "url": "https://github.com/skyra-project/archid-components/issues"
  },
  "engines": {
    "node": ">=16.9.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@discordjs/builders": "^0.12.0",
    "@sapphire/pieces": "^3.2.0",
    "discord-api-types": "^0.26.1",
    "fastify": "^3.27.1",
    "tslib": "^2.3.1",
    "tweetnacl": "^1.0.3"
  },
  "devDependencies": {
    "@sapphire/eslint-config": "^4.2.0",
    "@sapphire/prettier-config": "^1.3.0",
    "@sapphire/ts-config": "^3.3.0",
    "@types/node": "^17.0.15",
    "tsup": "^5.11.13",
    "typescript": "^4.5.5"
  }
}
